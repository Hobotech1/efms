                    <property
                      dataType="System.String"
                      description="The SQL command to be executed."
                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT s.[NationalEmisNumber] as EmistoPR
      ,s.[NationalEmisNumber]
      ,s.[OfficialName]
      ,s.[OtherName]
	  --assessment id and action date return lots of rows for one site - [dbo].[SiteAssessmentActivity] can have 1 to many relationship. validate business rule
	  ,s.[AssessmentDate]
	  ,s.[SchoolTypeID] 
	  ,s.[SchoolStatusID]
      ,s.[SiteSizeInHectores]
      ,s.[IsDoubleShiftSite]
	  ,s.[HasFormalBoundaries]
	  ,princPers.[FirstName] as PrincipalFirstNameorInitial-- v3 uses initials - must check of its necessary to get first letter since neims uses both names and initial in the same field
	  ,princPers.[Surname] as PrincipalSurname
	  ,persC.Detail AS PrincipalContactNumber-- all records in this table are typeid 1 so no need to do a join looking for cellphone
      ,pa.AddressLine1 as PhysicalAddressLine1, pa.AddressLine2 as PhysicalAddressLine2, pa.AddressLine3 as PhysicalAddressLine3, /*pa.AddressLine4 as PhysicalAddressLine4,*/ pa.PostalCode  as PhysicalPostalCode
	  ,po.AddressLine1 as PostalAddressLine1, po.AddressLine2 as PostalAddressLine2, /*po.AddressLine3 as PostalAddressLine3, po.AddressLine4 as PostalAddressLine4, */ po.PostalCode  as PostalPostalCode
	   ,s.[SiteOwnershipTypeID]
	  ,scEmail.Detail as SiteContactEmail, scFax.Detail as SiteContactFax, scPhone.Detail as SiteContactPHone -- may also do fax area and phone area using instr if required
	  ,s.[Longitude],s.[Latitude]
      ,s.[HighestGradeID],s.[LowestGradeID]
	  ,s.[LandOwnershipTypeID]
	  ,so.HasOwnerSignedAgreementWithMEC
	  , ownerp.firstname AS SiteOwnerContactName, ownerC.Detail as OwnerContactDetails
	  ,'NEIMS' as LastAssessmentBy -- defaulting to this until I figure a way to get max(date) within join --,u.[UserName] as AssessmentBy
	  -- could not locate [afr_Repair_Budget_Year]
      -- could not locate [afr_Repair_Cost_Property]
      -- could not locate [afr_Repair_Cost_School]
      -- could not locate [afr_RepairandReplaceCosTimestamp]
      -- could not locate [afr_Replace_Cost_Property]
      -- could not locate [afr_Replace_Cost_School]
	  , left(malestudsgradeR.Value, charindex('.', malestudsgradeR.Value)-1) as MaleLearnersGradeR
	  ,	left(femalestudsgradeR.Value, charindex('.', femalestudsgradeR.Value)-1) as FemaleLeanersGradeR 
	  , left(maleteacher.Value, charindex('.', maleteacher.Value) -1) as MaleTeachers
	  , left(femaleteacher.Value, charindex('.',  femaleteacher.Value) -1) as FemaleTeachers
	  ,case when platoon.PremiseSharingTypeID is null then 0 else 1 end IsPlatooningSchool 
      ,platoon.SharingWithNationalEmisNumber as PlatooningSchoolEmisNo
      ,case when tenant.PremiseSharingTypeID is null then 0 else 1 end IsTenantSchool
      ,tenant.SharingWithNationalEmisNumber as TenantingSchoolEmisNo
      ,case when host.PremiseSharingTypeID is null then 0 else 1 end IsHostingSchool
      ,host.SharingWithNationalEmisNumber as HostingSchoolEmisNo
	  -- this catchment area sql will cause return of multiple rows  select sc.SiteID, sc.CatchmentAreaID, area from [SiteCatchmentAreas] sc inner join [CatchmentAreas] ca on sc.CatchmentAreaID = ca.CatchmentAreaID order by sc.SiteID  
	  ,comm.InternerConnectivityForTeachingAndLearning 
      ,comm.InternerConnectivityForAdmin
	  -- could not locate construction date
	  -- could not locate sectorid
	  -- could not locate land owner other 
	  -- could not locate [MagesterialDistrict]
      -- could not locate [EDO_Name]
      -- could not locate [geoCensusSubID]
	  -- could not locate [geoDistrictMunicID]
      -- could not locate [geoEducBoundryID]
	/*  ,s.[MunicipalityID] this is not the right match for geoLocalmunic - foreign key reference check causes failure*/
	  -- could not locate [geoMunicipalWardID]
      -- could not locate [geoProvinceID]
      -- could not locate [IsEstimatedGPS]
      -- could not locate [geoEducationClusterID]
      -- could not locate [geoMagisterialDistrictID]
      -- could not locate [SubSector]
      -- could not locate [EducCircuit]
      -- could not locate [afr_Local_Municipality_ID]
      -- could not locate [date_last_modified]
      -- could not locate [BudgetProgrammeID]
      -- could not locate [EducCluster]
      -- could not locate [afr_revision_created]
      -- could not locate [afr_MasterlistGPS_Primary_East]
      -- could not locate [afr_MasterlistGPS_Primary_South]
      -- could not locate [afr_Doe_Circuit_ID]
      -- could not locate [afr_EDO_ID]
      -- could not locate [city_id]
      -- could not locate [county_id]
      -- could not locate [ctry_id]
      -- could not locate [afr_FCI]
      -- could not locate [afr_FCI_SEI_Mean]
      -- could not locate [description]
      -- could not locate [regn_id]
      -- could not locate [state_id]
      ,case when s.[SchoolTypeID] = '14' then 1 else 0 end IsIndependent
      -- could not locate [afr_WebSite]
	  , left(malestudsGr1to12.Value, charindex('.', malestudsGr1to12.Value) -1) as MaleLearnersGr1to12 
	  , left(femalestudeGr1to12.Value, charindex('.', femalestudeGr1to12.Value) -1) as FemaleLearnersGr1to12
	  -- could not locate [afr_PhoneArea_Alt]
      -- could not locate [afr_PhoneNumber_Alt]
      -- could not locate [afr_CatchmentAreas4]
      -- could not locate [afr_MasterlistQuintile]
      -- could not locate [afr_usage_type_property_ID]
	  -- could not locate [afr_NoDisabledLearners]
      -- could not locate [afr_NoHostelLearners]
	/*  ,s.LastUpdateDate*/
      ,0 as IsRevisionCreated /*destination column can't be null. using this for now*/
	 FROM [dbo].[Sites] s
	 INNER JOIN [dbo].[Principal] princ ON s.[SiteID] = princ.[SiteID]
	 LEFT OUTER JOIN [dbo].[Person] princPers ON princ.PersonID = princPers.PersonID
	 LEFT OUTER JOIN [dbo].[PersonContacts] persC ON princPers.PersonID = persC.PersonID
	 LEFT OUTER JOIN [dbo].[SiteContacts] scEmail ON s.SiteID = scEmail.SiteID AND scEmail.ContactTypeID = 4 -- email
	 LEFT OUTER JOIN [dbo].[SiteContacts] scFax ON s.SiteID = scFax.SiteID AND scFax.ContactTypeID = 3 -- fax
	 LEFT OUTER JOIN [dbo].[SiteContacts] scPhone ON s.SiteID = scPhone.SiteID AND scPhone.ContactTypeID = 2 -- phone
	 LEFT OUTER JOIN [dbo].[Site_Address] physicalAddr ON s.[SiteID] = physicalAddr.SiteID AND physicalAddr.AddressTypeID = 2 -- HOME 
	 LEFT OUTER JOIN [dbo].[Address] pa ON physicalAddr.AddressID = pa.AddressID
	 LEFT OUTER JOIN [dbo].[Site_Address] postalAddr ON s.[SiteID] = postalAddr.SiteID AND postalAddr.AddressTypeID = 1 -- POSTAL 
	 LEFT OUTER JOIN [dbo].[Address] po ON postalAddr.AddressID = po.AddressID
	 LEFT OUTER JOIN [dbo].[SiteOwner] so ON s.SiteID = so.SiteID
	 LEFT OUTER JOIN [dbo].[Person] ownerp on so.ContactPersonID = ownerp.PersonID
	 LEFT OUTER JOIN [dbo].[PersonContacts] ownerC ON ownerp.PersonID = ownerC.PersonID
	 --LEFT OUTER JOIN [dbo].[SiteAssessments] assess ON s.[NationalEmisNumber] = assess.[NationalEmisNumber]  -- one school can have multiple assessments, find out how to bring back results only on the last assessment
	 --LEFT OUTER JOIN [dbo].[Users] u ON assess.[ActionedByUserID] = u.[UserID]
	 LEFT OUTER JOIN [dbo].[SiteStats] malestudsgradeR on malestudsgradeR.SiteID = s.SiteID and malestudsgradeR.SiteStatsTypeID = 1 -- Male Learners (Grade R)
	 LEFT OUTER JOIN [dbo].[SiteStats] femalestudsgradeR on femalestudsgradeR.SiteID = s.SiteID and femalestudsgradeR.SiteStatsTypeID = 2 -- Female Learners (Grade R)
	 LEFT OUTER JOIN [dbo].[SiteStats] malestudsGr1to12 on malestudsGr1to12.SiteID = s.SiteID and malestudsGr1to12.SiteStatsTypeID = 3 -- Male Learners (Grade 1-12)
	 LEFT OUTER JOIN [dbo].[SiteStats] femalestudeGr1to12 on femalestudeGr1to12.SiteID = s.SiteID and femalestudeGr1to12.SiteStatsTypeID = 4 -- Female Learners (Grade 1-12)
	 LEFT OUTER JOIN [dbo].[SiteStats] maleteacher on maleteacher.SiteID = s.SiteID and maleteacher.SiteStatsTypeID = 5 -- Male Educators (Grade R included)
	 LEFT OUTER JOIN [dbo].[SiteStats] femaleteacher on femaleteacher.SiteID = s.SiteID and femaleteacher.SiteStatsTypeID = 6 -- Female Educators (Grade R included)
	 LEFT OUTER JOIN [dbo].[SitePremiseSharing] host on host.SharingWithNationalEmisNumber = s.NationalEmisNumber and host.PremiseSharingTypeID = 1  --1	Host School
	 LEFT OUTER JOIN [dbo].[SitePremiseSharing] tenant on tenant.SharingWithNationalEmisNumber = s.NationalEmisNumber and tenant.PremiseSharingTypeID = 2  --2	Tenant School
	 LEFT OUTER JOIN [dbo].[SitePremiseSharing] platoon on platoon.SharingWithNationalEmisNumber = s.NationalEmisNumber and platoon.PremiseSharingTypeID = 3  --3	Platoon School
	 LEFT OUTER JOIN [dbo].[SiteCommunicationSystemAvailable] comm on comm.SiteID = s.SiteID and comm.CommunicationSystemAvailableTypeID = 6 -- internet</property>
