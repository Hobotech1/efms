                      name="SqlCommand"
                      UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">Select s.NationalEmisNumber
           ,CASE
				WHEN sa.SiteAccessAvailabilityTypeID = 1 THEN 6
				WHEN sa.SiteAccessAvailabilityTypeID = 2 THEN 5
				WHEN sa.SiteAccessAvailabilityTypeID = 3 THEN 2
